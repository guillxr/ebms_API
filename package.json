{
  "name": "hemosync",
  "version": "1.0.0",
  "description": "API for managing blood supplies and stocks in emergency situations",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server",
    "dev": "nodemon server",
    "prepare": "husky install",
    "ci": "npx eslint . --ext .js",
    "format": "prettier --write .",
    "posinstall": "npx prisma generate"
  },
  "_moduleAliases": {
    "@src": "src",
    "@config": "src/config",
    "@controllers": "src/controllers",
    "@middlewares": "src/middlewares",
    "@models": "src/models",
    "@routes": "src/routes",
    "@services": "src/services",
    "@utils": "src/utils",
    "@validators": "src/validators"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.7.0",
    "bcrypt": "^5.1.1",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "prisma": "^6.7.0"
  }
}
